{
  "name": "@mpecarina/koa-template",
  "version": "1.4.2",
  "description": "koa typescript template",
  "keywords": [
    "koa",
    "template",
    "docker"
  ],
  "main": "dist/index.js",
  "types": "types/koa-template.d.ts",
  "repository": {
    "url": "https://github.com/mpecarina/koa-template"
  },
  "scripts": {
    "build": "rm -rf dist/ && ./node_modules/typescript/bin/tsc && ./node_modules/prettier/bin-prettier.js --write 'dist/**/*.js'",
    "dev": "nodemon --watch 'src/**/*.ts' --watch 'routes.json' --watch 'types/*.d.ts' --exec './node_modules/ts-node/dist/bin.js' src/index.ts",
    "lint": "./node_modules/eslint/bin/eslint.js src/**/*.ts --fix",
    "publish:cd": "yarn lint && yarn build && yarn publish",
    "release:patch": "yarn lint && yarn build && release patch",
    "release:minor": "yarn lint && yarn build && release minor",
    "release:major": "yarn lint && yarn build && release major",
    "start": "node dist/index.js",
    "test": "jest test/unit --verbose --passWithNoTests"
  },
  "author": "mpecarina",
  "license": "MIT",
  "dependencies": {
    "koa": "^2.12.0",
    "koa-bodyparser": "^4.3.0",
    "koa-json": "^2.0.2",
    "koa-passport": "^4.1.1",
    "koa-router": "^8.0.8",
    "koa-session": "^6.0.0",
    "koa-static-server": "^1.5.2",
    "koa2-winston": "^3.1.1",
    "prom-client": "^12.0.0",
    "prometheus-api-metrics": "^2.2.5",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-passport": "^4.0.2",
    "@types/koa-router": "^7.4.1",
    "@types/koa-session": "^5.10.2",
    "@types/koa-static-server": "^1.3.0",
    "@types/node": "^14.0.3",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "release-script": "^1.0.2",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.9.2"
  }
}
